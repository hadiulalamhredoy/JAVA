
package basicjava;

public class String04 {
    
    public static void main(String [] args){ 
    String s1 = "This is my Country"; 
     System.out.println(s1); 

     String s2 = s1.replace('i','j'); 
     System.out.println(s2); 

     String [] s3 = s1.split(" "); 

     for (String x:s3){          
       System.out.println(x); 
} 

    } 
}
    
    

