
package begginnerjava;
import java.util.Scanner;
public class BitwiseOperator {
    
    public static void main(String [] args){
        
        int a = 32;
        int b = 23;
        
        int c;
        
        c = a<<3;
        System.out.println("a<<3 = " +c);
        
        c = a>>3;
        System.out.println("a>>3 = " +c);
        
        
        
    }
    
}
