   
import java.util.Scanner;


 class Main {
   public static void main(String args[]) {
       Scanner input = new Scanner(System.in);
       int m,n;
       System.out.print("Enter the m  Number  : ");
        m = input.nextInt();
       System.out.print("Enter the n  Number  : ");
       n = input.nextInt();
       
       for(int i=0; i<n; i++) {
           for(int j=0; j<m; j++) {
               System.out.print("#");
           }
           System.out.println();
       }
   }
}
